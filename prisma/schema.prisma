generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Deck {
  id       Int       @id @default(autoincrement())
  title    String
  problems Problem[]
}

model Problem {
  id     Int    @id @default(autoincrement())
  front  String
  back   String
  deckId Int
  deck   Deck   @relation(fields: [deckId], references: [id])
}
